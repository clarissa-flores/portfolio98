<section class="console-shell" data-console>
  <div class="console-output" aria-live="polite"></div>
  <form class="console-input-row">
    <label for="console-input" class="sr-only">Console input</label>
    <span class="console-prompt">&gt;</span>
    <input id="console-input" name="console-input" autocomplete="off" />
    <button type="submit">Run</button>
  </form>
</section>
